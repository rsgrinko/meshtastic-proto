{
  "name": "rsgrinko/meshtastic-protobufs",
  "type": "library",
  "description": "PHP Compiled Protobufs from the Official Meshtastic Protobufs repository",
  "keywords": ["meshtastic", "protobuf", "rsgrinko"],
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Grinko",
      "email": "rsgrinko@gmail.com"
    }
  ],
  "require": {
    "google/protobuf": "*",
    "protobuf-php/protobuf": "*"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "Meshtastic\\": "src/Meshtastic/",
      "GPBMetadata\\": "src/GPBMetadata/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
